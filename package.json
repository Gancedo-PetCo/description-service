    "artillery": "^1.6.1",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.2.0",
